    "redhat.telemetry.enabled": false,
    "terminal.external.osxExec": "iTerm.app",
    "julia.symbolCacheDownload": true,
    "git.autofetch": true,
    "juliaFormatter.style": "blue",
    "editor.inlineSuggest.enabled": false,
    "editor.unicodeHighlight.allowedCharacters": {
        "ℓ": true,
        "σ": true,
        "α": true,
        "ρ": true,
        "γ": true
