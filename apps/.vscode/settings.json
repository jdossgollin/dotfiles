    "editor.fontSize": 13,
    "editor.fontFamily": "'JuliaMono', 'Fira Code', 'Fira Mono for Powerline', 'Source Code Pro', monospace",
    "editor.fontLigatures": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "python.pythonPath": "/usr/local/Caskroom/miniconda/base/envs/salib/bin/python",
    "markdownlint.config": {
        "default": true,
        "MD007": false,
        "no-hard-tabs": false,
        "line-length": false
    },
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "latex-workshop.latexindent.path": "/Users/James/.dotfiles/bin/latexindent",
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
    "todo-tree.general.enableFileWatcher": true,
    "todo-tree.general.tags": [
        "BUG",
        "FIXME",
        "TODO",
        "XXX",
        "CITE",
        "CREF"
    ],
    "todo-tree.highlights.highlightDelay": 10,
    "todo-tree.filtering.excludeGlobs": [
        "\"*.pdf\""
    ],
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "latex-workshop.chktex.delay": 100,
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\\\*|^|%|\\()\\\\s*($TAGS)|^\\\\s*- \\\\[ \\\\])",
    "vsnotes.defaultNotePath": "/Users/james/Documents/Github/notes",
    "vsnotes.templates": [
        "meeting"
    ],
    "sync.autoUpload": true,
    "sync.autoDownload": true,
    "terminal.integrated.inheritEnv": false,
    "sync.gist": "03e4bb2376f645bc8b92a2dc3fa409db",
    "latex-workshop.intellisense.update.delay": 500,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.message.badbox.show": false,
    "latex-workshop.message.latexlog.exclude": [
        "\\hbox"
    ],
    "cSpell.ignoreRegExpList": [
        "\"\\\\\\\\\\\\w*(\\\\[.*?\\\\])?(\\\\{.*?\\\\})?\"",
        "\"\\\\$.+?\\\\$\""
    ],
    "files.exclude": {
        "*.aux": true,
        "*.lof": true
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        }
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.enableFiletypes": [
        "!python",
        "dockerfile",
        "jupyter",
        "pip-requirements",
        "r",
        "snakemake",
        "tex"
    ],
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "latex-workshop.docker.image.latex": "jdossgollin/latex",
    "julia.enableTelemetry": true,
    "python.linting.enabled": true,
    "editor.wordWrapColumn": 80,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "workbench.colorTheme": "Atom One Dark",
    "[julia]": {
        "editor.defaultFormatter": "julialang.language-julia",
    },
    "julia.NumThreads": 4,
    "markdown-preview-enhanced.breakOnSingleNewLine": false,
    "markdown-preview-enhanced.enableHTML5Embed": true,
    "julia.execution.resultType": "both",
    "jupyter.sendSelectionToInteractiveWindow": true,
    "jupyter.askForKernelRestart": false,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
}