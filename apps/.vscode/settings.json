    "editor.fontSize": 13,
    "editor.fontFamily": "'Fira Code', 'Fira Mono for Powerline', 'Source Code Pro', monospace",
    "editor.fontLigatures": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "vsicons.dontShowNewVersionMessage": true,
    "markdownlint.config": {
        "default": true,
        "MD007": false,
        "no-hard-tabs": false,
        "line-length": false
    },
    "editor.formatOnSave": true,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.filtering.useBuiltInExcludes": "file and search excludes",
    "todo-tree.general.enableFileWatcher": true,
    "todo-tree.general.tags": [
        "BUG",
        "FIXME",
        "TODO",
        "XXX",
        "CITE",
        "CREF"
    ],
    "todo-tree.highlights.highlightDelay": 10,
    "todo-tree.filtering.excludeGlobs": [
        "*.pdf"
    ],
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "todo-tree.regex.regex": "((//|#|<!--|;|/\\\\*|^|%|\\()\\\\s*($TAGS)|^\\\\s*- \\\\[ \\\\])",
    "vsnotes.defaultNotePath": "/Users/james/Documents/Github/notes",
    "vsnotes.templates": [
        "meeting"
    ],
    "terminal.integrated.inheritEnv": false,
    "latex-workshop.intellisense.update.delay": 500,
    "latex-workshop.intellisense.package.enabled": true,
    "latex-workshop.message.badbox.show": "none",
    "latex-workshop.message.latexlog.exclude": [
        "\\hbox"
    ],
    "cSpell.ignoreRegExpList": [
        "\"\\\\\\\\\\\\w*(\\\\[.*?\\\\])?(\\\\{.*?\\\\})?\"",
        "\"\\\\$.+?\\\\$\""
    ],
    "files.exclude": {
        "*.aux": true,
        "*.lof": true
    },
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "lualatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-lualatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "xelatexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-xelatex",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "latexmk_rconly",
            "command": "latexmk",
            "args": [
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ],
            "env": {}
        },
        {
            "name": "rnw2tex",
            "command": "Rscript",
            "args": [
                "-e",
                "knitr::opts_knit$set(concordance = TRUE); knitr::knit('%DOCFILE_EXT%')"
            ],
            "env": {}
        },
        {
            "name": "tectonic",
            "command": "tectonic",
            "args": [
                "-X",
                "compile",
                "%DOC%.tex",
                "--synctex",
                "--keep-logs",
                "--keep-intermediates"
            ],
            "env": {}
        }
    ],
    "cSpell.allowCompoundWords": true,
    "cSpell.enabledFileTypes": {
        "python": false,
        "dockerfile": true,
        "jupyter": true,
        "pip-requirements": true,
        "plaintext": true,
        "r": true,
        "snakemake": true,
        "tex": true
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font",
    "latex-workshop.docker.image.latex": "tianon/latex",
    "julia.enableTelemetry": true,
    "editor.wordWrapColumn": 80,
    "files.associations": {
        "*.rmd": "markdown"
    },
    "workbench.colorTheme": "Atom One Dark",
    "[julia]": {
